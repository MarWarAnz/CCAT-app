<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" maxWidth="850.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ccat_view.MainMenu.MainMenuController">
   <children>
      <BorderPane layoutX="1.0">
         <center>
            <TabPane fx:id="tabs" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
              <tabs>
                <Tab fx:id="partA" text="Part A: Metadata">
                     <content>
                        <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" prefWidth="850.0">
                          <content>
                            <AnchorPane>
                                 <children>
                                    <VBox fx:id="partAScroller" prefHeight="425.0" prefWidth="640.0" />
                                 </children>
                              </AnchorPane>
                          </content>
                        </ScrollPane>
                     </content>
                  </Tab>
                <Tab fx:id="partB" text="Part B">
                     <content>
                        <ScrollPane hbarPolicy="NEVER" vbarPolicy="ALWAYS">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="426.0" prefWidth="640.0">
                                 <children>
                                    <VBox fx:id="partBScroller" prefHeight="428.0" prefWidth="640.0" />
                                 </children>
                              </AnchorPane>
                          </content>
                        </ScrollPane>
                     </content>
                  </Tab>
                  <Tab fx:id="partC" text="Part C">
                     <content>
                        <ScrollPane hbarPolicy="NEVER">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="425.0" prefWidth="640.0">
                                 <children>
                                    <VBox fx:id="partCScroller" prefHeight="427.0" prefWidth="640.0" />
                                 </children>
                              </AnchorPane>
                          </content>
                        </ScrollPane>
                     </content>
                  </Tab>
                  <Tab text="Admin">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
              </tabs>
            </TabPane>
         </center>
         <top>
            <MenuBar BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#onAdminTasks" text="Admin Tasks" />
                        <MenuItem mnemonicParsing="false" onAction="#onLogout" text="Logout" />
                        <MenuItem mnemonicParsing="false" onAction="#onExit" text="Exit" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#onAbout" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
         <bottom>
            <Pane prefHeight="52.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <children>
                  <Button layoutX="464.0" layoutY="14.0" mnemonicParsing="false" onAction="#saveFile" prefHeight="25.0" prefWidth="61.0" text="Submit" />
                  <Button layoutX="534.0" layoutY="14.0" mnemonicParsing="false" onAction="#onExit" text="Cancel" />
               </children>
            </Pane>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
